{
  "$schema": "vscode://schemas/color-theme",
  "name": "Reify Dark",
  "type": "dark",
  "colors": {
    "activityBarBadge.background": "#007acc",
    "editor.background": "#090920",
    "editor.foreground": "#f8f8f8",
    "editor.inactiveSelectionBackground": "#3a3d41",
    "editor.selectionBackground": "#dcb77440",
    "editor.selectionHighlightBackground": "#a39ab540",
    "editorGroup.border": "#f8f8f8",
    "editorIndentGuide.activeBackground": "#707070",
    "editorIndentGuide.background": "#404040",
    "gitDecoration.conflictingResourceForeground": "#ec5948",
    "gitDecoration.modifiedResourceForeground": "#f8f8f8",
    "gitDecoration.submoduleResourceForeground": "#f8f8f8",
    "gitDecoration.ignoredResourceForeground": "#A9A3B8",
    "gitDecoration.addedResourceForeground": "#f8f8f8",
    "gitDecoration.untrackedResourceForeground": "#f8f8f8",
    "input.placeholderForeground": "#a6a6a6",
    "list.dropBackground": "#383b3d",
    "menu.background": "#252526",
    "menu.foreground": "#cccccc",
    "notebook.selectedCellBackground": "#383b3d50",
    "panel.border": "#f8f8f8",
    "sideBar.background": "#090920",
    "sideBar.border": "#f8f8f8",
    "sideBar.foreground": "#f8f8f8",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#cccccc33",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#090920",
    "statusBar.border": "#f8f8f8",
    "statusBar.noFolderBackground": "#090920",
    "statusBar.noFolderBorder": "#f8f8f8",
    "statusBar.debuggingBackground": "#090920",
    "statusBarItem.remoteBackground": "#16825d",
    "statusBarItem.remoteForeground": "#f8f8f8",
    "tab.lastPinnedBorder": "#cccccc33",
    "terminal.ansiBlack": "#3c3d37",
    "terminal.ansiBlue": "#80b1d3",
    "terminal.ansiBrightBlack": "#75715e",
    "terminal.ansiBrightBlue": "#3ca1e7",
    "terminal.ansiBrightCyan": "#3defd1",
    "terminal.ansiBrightGreen": "#97d52b",
    "terminal.ansiBrightMagenta": "#e867de",
    "terminal.ansiBrightRed": "#ec5948",
    "terminal.ansiBrightWhite": "#feffff",
    "terminal.ansiBrightYellow": "#e48d2b",
    "terminal.ansiCyan": "#8dd3c7",
    "terminal.ansiGreen": "#b3de69",
    "terminal.ansiMagenta": "#ce9aca",
    "terminal.ansiRed": "#fb8072",
    "terminal.ansiWhite": "#f8f8f8",
    "terminal.ansiYellow": "#fdb462",
    "terminal.border": "#f8f8f8",
    "terminal.foreground": "#f8f8f8",
    "terminal.selectionBackground": "#dcb77440"
  },
  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A9A3B8"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": {
        "foreground": "#B3DE69"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#EC5948"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#3CA1E7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#3CA1E7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#B3DE69"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#B3DE69"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": ["storage.modifier", "keyword.operator.noexcept"],
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": ["string", "meta.embedded.assembly"],
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FB8072"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#B3DE69"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#B3DE69"
      }
    },
    {
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#8DD3C7"
      }
    },
    {
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#8DD3C7"
      }
    },
    {
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#8DD3C7"
      }
    },
    {
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator",
        "entity.name.operator"
      ],
      "settings": {
        "foreground": "#E867DE"
      }
    },
    {
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": ["variable.other.constant", "variable.other.enummember"],
      "settings": {
        "foreground": "#4FC1FF"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": {
        "foreground": "#FB8072"
      }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": {
        "foreground": "#8DD3C7"
      }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "constant.character",
      "settings": {
        "foreground": "#3CA1E7"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FDB462"
      }
    },
    {
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#C8C8C8"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#EC5948"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
